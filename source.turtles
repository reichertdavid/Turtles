CANVAS 1000 1000

ALLOCINT WHITE 0xFFFFFF
ALLOCINT XPOS 0
ALLOCINT YPOS 0

SETCOLOR i$WHITE
PUSH

FORLOOP DEG 0 9000
FORLOOP SIZE 0 500

COSINT XPOS i$DEG i$SIZE
ADDINT XPOS 500

SININT YPOS i$DEG i$SIZE
ADDINT YPOS 500

SETPOS i$XPOS i$YPOS

ENDFORLOOP
ENDFORLOOP

SETPOS i$XPOS i$YPOS

ENDFORLOOP

POP
FINISH
